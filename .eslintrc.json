{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": "off",
    "no-await-in-loop": "off",
    "quotes": "off",
    "comma-dangle": "off",
    "no-plusplus": "off"
  }
}
