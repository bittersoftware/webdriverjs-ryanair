{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": "off",
    "no-await-in-loop": "off",
    "quotes": "off",
    "comma-dangle": "off",
    "no-plusplus": "off",
    "no-console": "off",
    "operator-linebreak": "off",
    "object-curly-newline": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
